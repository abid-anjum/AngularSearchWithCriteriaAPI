spring:
  datasource:
    url: jdbc:mysql://localhost:3306/demodb?serverTimezone=UTC
    username: root
    password: 
    driver-class-name: com.mysql.cj.jdbc.Driver
  jpa:
    properties:
      hibernate:
        show_sql: true
        format_sql: true
logging:
  file: "C:/os/application.log"
  pattern:
    console: "%d %-5level %logger : %msg%n"
    file: "%d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%"
  level:
    ROOT: DEBUG
    com.aisc.angular.search: DEBUG
    org:
      springframework.web: ERROR
      hibernate:
        SQL:
          TRACE
        type:
          descriptor:
            sql:
              BasicBinder:
                TRACE
h2:
    console:
    enabled: true
server:
  port: 8081